# AWS S3 Configuration
# Add these to your .env file in the backend directory

# AWS S3 Settings
AWS_S3_BUCKET_NAME=referconnect-resumes
AWS_S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key

# Frontend S3 Configuration (add to your frontend .env file)
REACT_APP_S3_BUCKET_NAME=referconnect-resumes
REACT_APP_S3_REGION=us-east-1
REACT_APP_S3_ACCESS_KEY_ID=your-aws-access-key-id
REACT_APP_S3_SECRET_ACCESS_KEY=your-aws-secret-access-key
